generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum PlanType {
  free
  premium
}

model User {
  id        String   @id @default(uuid())
  name      String?
  email     String?  @unique
  image     String?
  plan      PlanType   @default(free)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

